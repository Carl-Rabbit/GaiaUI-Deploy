  animation: {
    comment: 'You can create and use parameters and template locally to simplify the animation definition.',
    params: {
      wealthySelExpr: '@data.category >= 0.9',
      poorSelExpr: '@data.category <= 0.2',
      middleSelExpr: '0.3 <= @data.category && @data.category < 0.7',
      transitionSelExpr: '0.2 <= @data.category && @data.category < 0.9',
    },
    main: {
      sync: [
        {
          ref: 'BarChartEnter',
          axisTickAnimation: {
            ref: 'Grow',
            from: 'top',
            ease: 'circ.inOut',
          },
        },
        {
          ref: 'Fly',
          target: '.Annotation',
          from: 'left',
          offset: 3,
        },
        {
          ref: 'MyRangeBounce',
          barSelExpr: '$wealthySelExpr',
          barLabelSel: '#bar-label-3',
          offset: 7,
        },
        {
          ref: 'MyRangeBounce',
          barSelExpr: '$poorSelExpr',
          barLabelSel: '#bar-label-0',
          offset: 9,
        },
        {
          ref: 'ChangeColor',
          target: [
            { filter: '.Bar' },
            { filterExpr: '$poorSelExpr' },
          ],
          fill: '#ffffff',
          stagger: 0.02,
          yoyo: true,
          offset: 13,
          duration: 3,
        },
        {
          ref: 'MoveTo',
          target: '.Annotation',
          x: '+=60px',
          yoyo: true,
          offset: 15.5,
        },
        {
          ref: 'MyRangeBounce',
          barSelExpr: '$middleSelExpr',
          barLabelSel: '#bar-label-1',
          offset: 19,
        },
        {
          target: [
            { filter: '.Bar' },
            { filterExpr: '$transitionSelExpr' },
          ],
          groupBy: 'parseInt(@data.category * 10)',
          foreach: {  ref: 'Bounce' },
          stagger: 0.4,
          offset: 21,
        },
      ],
    },
  },
  templates: [
    {
      name: 'MyRangeBounce',
      params: {
        barSelExpr: { type: "string" },
        barLabelSel: { type: "string" },
      },
      main: {
        sync: [
          {
            ref: 'Bounce',
            target: [
              { filter: '.Bar' },
              { filterExpr: '$barSelExpr' }
            ],
          },
          {
            ref: 'Bounce',
            target: '$barLabelSel',
            offset: 0.4
          }
        ],
      }
    },
    {
      name: 'BarChartEnter',
      params: {
        titleStagger: 0.02,
        titleDuration: 2,
        componentDuration: 2,
        axisLabelStagger: 0.04,
        axisTickAnimation: {
          type: 'aniUnit',
          value: { ref: 'Empty' }
        },
        barLabelAnimation: {
          type: 'aniUnit',
          value: {
            ref: 'Type',
            effect: 'fade'
          }
        }
      },
      main: {
        sync: [
          {
            ref: 'Type',
            target: '.Title',
            typeStagger: '$titleStagger',
            effect: 'fade',
            duration: '$titleDuration'
          },
          {
            concat: [
              {
                ref: 'PathDraw',
                target: '.XAxis.Domain',
                ease: 'circ.inOut',
                duration: '$componentDuration',
              },
              {
                sync: [
                  {
                    ref: 'PathDraw',
                    target: '.YAxis.Domain',
                    ease: 'circ.inOut',
                  },
                  {
                    target: '.Axis.Tick',
                    child: '$axisTickAnimation',
                  },
                  {
                    ref: 'Wipe',
                    target: '.Bar',
                    ease: 'circ.inOut',
                  }
                ],
                duration: '$componentDuration'
              },
              {
                ref: 'Type',
                target: '.Axis.Label',
                typeStagger: '$axisLabelStagger',
                effect: 'fade',
                duration: '$componentDuration'
              },
              {
                target: '.BarLabel',
                child: '$barLabelAnimation',
              }
            ]
          },
        ]
      }
    },
    {
      name: 'Bounce',
      params: {
        distance: 20,
      },
      main: {
        ref: 'MoveTo',
        y: '-=$distance',
        yoyo: true,
      }
    },
  ]
}
